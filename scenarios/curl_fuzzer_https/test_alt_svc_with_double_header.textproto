    payload: "HTTP/1.1 200 OK\r\nHost: server.example.com\r\nAlt-Svc: h3-23=\x22something.example.com:9999\x22\r\nAlt-Svc: h2=\x22alt.example.com:443\x22, h2=\x22:443\x22\r\nConnection: Keep-Alive\r\n\r\n"
  }
